    "height = float(\"6.5\")\n",
    "radius = float(\"3.5\")\n",
    "height_cm = height * 2.54\n",
    "radius_cm = radius * 2.54\n",
    "x = height_cm * radius_cm\n",
    "mug_volume = math.floor(x)\n",
    "mug_volume = str(mug_volume)\n",
    "print(\"I would like exactly \" + mug_volume + \"cc’s of your finest joe.\")\n"
